{
  "dashboard": {
    "title": "Prefect Flow Monitoring",
    "tags": ["mlops", "prefect", "orchestration"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Flow Run Status",
        "type": "stat",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "flow_run_status{flow_name=\"training_flow\"}",
            "legendFormat": "Training Flow",
            "refId": "A"
          },
          {
            "expr": "flow_run_status{flow_name=\"prediction_flow\"}",
            "legendFormat": "Prediction Flow",
            "refId": "B"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "Failed"},
              {"type": "value", "value": "1", "text": "Success"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "unit": "none"
          }
        }
      },
      {
        "id": 2,
        "title": "Flow Run Success Rate (24h)",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(training_count_total{status=\"success\"}) / sum(training_count_total) * 100",
            "legendFormat": "Training",
            "refId": "A"
          },
          {
            "expr": "sum(prediction_count_total{status=\"success\"}) / sum(prediction_count_total) * 100",
            "legendFormat": "Prediction",
            "refId": "B"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Training Flow Runs Over Time",
        "type": "timeseries",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "rate(training_count_total{status=\"success\"}[1h]) * 3600",
            "legendFormat": "Success - Horizon {{horizon}}",
            "refId": "A"
          },
          {
            "expr": "rate(training_count_total{status=\"failure\"}[1h]) * 3600",
            "legendFormat": "Failure - Horizon {{horizon}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "fillOpacity": 80,
              "stacking": {
                "mode": "normal"
              }
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*Failure.*"},
              "properties": [
                {
                  "id": "color",
                  "value": {"mode": "fixed", "fixedColor": "red"}
                }
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*Success.*"},
              "properties": [
                {
                  "id": "color",
                  "value": {"mode": "fixed", "fixedColor": "green"}
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 4,
        "title": "Prediction Flow Runs Over Time",
        "type": "timeseries",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(prediction_count_total{status=\"success\"}[1h]) * 3600",
            "legendFormat": "Success",
            "refId": "A"
          },
          {
            "expr": "rate(prediction_count_total{status=\"failure\"}[1h]) * 3600",
            "legendFormat": "Failure",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "fillOpacity": 80,
              "stacking": {
                "mode": "normal"
              }
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Failure"},
              "properties": [
                {
                  "id": "color",
                  "value": {"mode": "fixed", "fixedColor": "red"}
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Success"},
              "properties": [
                {
                  "id": "color",
                  "value": {"mode": "fixed", "fixedColor": "green"}
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          }
        }
      },
      {
        "id": 5,
        "title": "Training Flow Duration",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(training_duration_seconds_bucket[1h]))",
            "legendFormat": "p95 - Horizon {{horizon}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(training_duration_seconds_bucket[1h]))",
            "legendFormat": "p50 - Horizon {{horizon}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 6,
        "title": "Prediction Flow Duration",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 600, "color": "yellow"},
                {"value": 900, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Total Training Runs by Horizon",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 34},
        "targets": [
          {
            "expr": "sum by (horizon) (training_count_total)",
            "legendFormat": "Horizon {{horizon}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Total Prediction Runs",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 34},
        "targets": [
          {
            "expr": "sum(prediction_count_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Task Failure Rate by Step",
        "type": "table",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 42},
        "targets": [
          {
            "expr": "sum by (horizon, status) (training_count_total)",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "job": true, "instance": true},
              "indexByName": {},
              "renameByName": {
                "horizon": "Horizon",
                "status": "Status",
                "Value": "Count"
              }
            }
          }
        ],
        "options": {
          "showHeader": true
        }
      },
      {
        "id": 10,
        "title": "Data Rows Processed",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 52},
        "targets": [
          {
            "expr": "rate(data_rows_processed[5m]) * 60",
            "legendFormat": "{{step}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          }
        }
      },
      {
        "id": 11,
        "title": "Last Training Run by Horizon",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 52},
        "targets": [
          {
            "expr": "last_training_timestamp",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "job": true, "instance": true},
              "indexByName": {},
              "renameByName": {
                "horizon": "Horizon",
                "Value": "Timestamp"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Timestamp"},
              "properties": [
                {
                  "id": "unit",
                  "value": "dateTimeFromNow"
                }
              ]
            }
          ]
        }
      }
    ]
  }
}


