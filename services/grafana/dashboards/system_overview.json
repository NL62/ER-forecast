{
  "dashboard": {
    "title": "System Overview",
    "tags": ["mlops", "system", "infrastructure"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "System Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"mlops-app-metrics\"}",
            "legendFormat": "MLOps App Status",
            "refId": "A"
          },
          {
            "expr": "up{job=\"prometheus\"}",
            "legendFormat": "Prometheus Status",
            "refId": "B"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Training Flow Status",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "flow_run_status{flow_name=\"training_flow\"}",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "mappings": [
              {"type": "value", "value": "0", "text": "Failed"},
              {"type": "value", "value": "1", "text": "Success"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "unit": "none"
          }
        }
      },
      {
        "id": 3,
        "title": "Prediction Flow Status",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "flow_run_status{flow_name=\"prediction_flow\"}",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "mappings": [
              {"type": "value", "value": "0", "text": "Failed"},
              {"type": "value", "value": "1", "text": "Success"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "unit": "none"
          }
        }
      },
      {
        "id": 4,
        "title": "Training Runs (Last 7 Days)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(training_count_total{status=\"success\"}[1h])",
            "legendFormat": "Success - Horizon {{horizon}}",
            "refId": "A"
          },
          {
            "expr": "rate(training_count_total{status=\"failure\"}[1h])",
            "legendFormat": "Failure - Horizon {{horizon}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Prediction Runs (Last 7 Days)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "rate(prediction_count_total{status=\"success\"}[1h])",
            "legendFormat": "Success",
            "refId": "A"
          },
          {
            "expr": "rate(prediction_count_total{status=\"failure\"}[1h])",
            "legendFormat": "Failure",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Last Training Timestamp by Horizon",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "last_training_timestamp",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "job": true, "instance": true},
              "indexByName": {},
              "renameByName": {
                "horizon": "Horizon",
                "Value": "Last Training (Unix)"
              }
            }
          }
        ]
      },
      {
        "id": 7,
        "title": "Last Prediction Timestamp",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "last_prediction_timestamp",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "dateTimeFromNow"
          }
        }
      },
      {
        "id": 8,
        "title": "Data Processing Duration",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(data_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "{{step}} (p95)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(data_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "{{step}} (p50)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Weather API Response Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(weather_api_response_time_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(weather_api_response_time_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 10,
        "title": "Weather API Success Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "sum(rate(weather_api_requests_total{status=\"success\"}[5m])) / sum(rate(weather_api_requests_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      }
    ]
  }
}


